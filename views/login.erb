<!-- <section id="contact" class="section section-contact scrollspy"> -->
<form  method="POST" action="/user/login">
    <div class="container center-align">
      <div class="row">
       <div class="col s12 m6 offset-l3 l6">
          <div class="card-panel grey lighten-3">
            <h5>Please fill out this form</h5>
             <div class="input-field">
              <input type="email" placeholder="Email" name="email" id="email" required>
              <label for="email">Email</label>
              </div>
              <div class="input-field"> 
              <input type="password" placeholder="Password" name="password" id="password-field"  required>
              <label for="password">Password</label>
              <i class="fa fa-eye field-icon toggle-password" style="font-size:16px;color:red" toggle="#password-field"></i>
 
            </div>
            <input type="submit" value="Submit" class="btn">
          </div>
        </div>
      </div>
    </form>
